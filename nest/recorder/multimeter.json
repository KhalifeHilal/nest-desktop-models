{
  "abbreviation": "mm",
  "elementType": "recorder",
  "id": "multimeter",
  "label": "multimeter",
  "params": [
    {
      "component": "tickSlider",
      "id": "interval",
      "label": "time interval of recording",
      "rules": [
        [
          "value < 1",
          "A small recording interval produces many data points which could cause a high system load and thus freezes and lags!",
          "warning"
        ]
      ],
      "ticks": [
        0.01,
        0.1,
        1,
        10
      ],
      "unit": "ms",
      "value": 1
    },
    {
      "component": "valueSlider",
      "id": "start",
      "label": "start time",
      "max": 10000,
      "min": 0,
      "step": 10,
      "unit": "ms",
      "value": 0
    },
    {
      "component": "valueSlider",
      "id": "stop",
      "label": "stop time",
      "max": 10000,
      "min": 0,
      "step": 10,
      "unit": "ms",
      "value": 10000
    }
  ]
}